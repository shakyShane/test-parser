{
  "name": "htmlparser2",
  "description": "Fast & forgiving HTML/XML/RSS parser",
  "version": "3.9.0",
  "author": "Felix Boehm <me@feedic.com>",
  "keywords": [
    "html",
    "parser",
    "streams",
    "xml",
    "dom",
    "rss",
    "feed",
    "atom"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/fb55/htmlparser2.git"
  },
  "bugs": {
    "mail": "me@feedic.com",
    "url": "http://github.com/fb55/htmlparser2/issues"
  },
  "directories": {
    "lib": "lib/"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lcov": "istanbul cover _mocha --report lcovonly -- -R spec",
    "coveralls": "npm run lint && npm run lcov && (cat coverage/lcov.info | coveralls || exit 0)",
    "test": "mocha && npm run lint",
    "lint": "jshint lib test && jscs lib test"
  },
  "dependencies": {
    "domelementtype": "^1.3.0",
    "domhandler": "^2.3.0",
    "domutils": "^1.5.1",
    "entities": "^1.1.1",
    "readable-stream": "^2.0.2"
  },
  "devDependencies": {
    "chai": "3.4.1",
    "coveralls": "^2.11.4",
    "handlebars": "4.0.5",
    "istanbul": "^0.3.18",
    "jscs": "^2.1.0",
    "jsdom": "7.2.1",
    "jshint": "^2.8.0",
    "mocha": "2.3.4",
    "mocha-lcov-reporter": "^0.0.2"
  },
  "browser": {
    "readable-stream": false
  },
  "license": "MIT",
  "jshintConfig": {
    "eqeqeq": true,
    "freeze": true,
    "latedef": "nofunc",
    "noarg": true,
    "nonbsp": true,
    "quotmark": "double",
    "undef": true,
    "unused": true,
    "trailing": true,
    "eqnull": true,
    "proto": true,
    "smarttabs": true,
    "node": true,
    "globals": {
      "describe": true,
      "it": true
    }
  }
}
